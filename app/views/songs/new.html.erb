<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.label :song_notes_attributes_0_content %>
  <input id="song_notes_attributes_0_content" name="song[notes_attributes][]">
  <%= f.label :song_notes_attributes_1_content %>
  <input id="song_notes_attributes_1_content" name="song[notes_attributes][]">

  <%= f.submit %>
<% end %>


